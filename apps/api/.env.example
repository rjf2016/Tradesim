# Database
DATABASE_URL=postgresql://tradesim:tradesim_dev@localhost:5432/tradesim

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-in-production

# Alpha Vantage (get your free key at https://www.alphavantage.co/support/#api-key)
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-api-key
ALPHA_VANTAGE_MAX_DAILY_REQUESTS=25

# Mock Data
# Set to true to use fake stock data (no API calls) - great for development
# Set to false to use real Alpha Vantage API
USE_MOCK_STOCK_DATA=true

# Cache TTL in milliseconds
# Default: 4 hours (14400000) in dev with mock data, 1 minute (60000) in production
# STOCK_CACHE_TTL_MS=14400000

# Server
PORT=3000
NODE_ENV=development
